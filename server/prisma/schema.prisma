generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Data {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  end_year   String
  intensity  String
  sector     String
  topic      String
  insight    String
  url        String
  region     String
  start_year String
  impact     String
  added      String
  published  String
  country    String
  relevance  String
  pestle     String
  source     String
  title      String
  likelihood String
}
